            <string>/// Do damamge
if (!attacked &amp;&amp; other.state != skeleton_roll_state) {
    audio_play_sound(snd_hit_two, 6, false);
    SkeletonStats.hp -= damage;
    other.hspd = sign(other.x-x)*knockback;
    other.image_xscale = sign(other.hspd)*-1;
    other.state = other.knockback_state;
    Camera.shake = knockback;
    Camera.alarm[0] = knockback;
    other.is_friction = true;
    var flash = instance_create(0, 0, Flash);
    flash.image_alpha = .5;
    
    // Check for death
    if (SkeletonStats.hp &lt;= 0) {
        with (other) instance_destroy();
        for (var i=0; i&lt;sprite_get_number(spr_skeleton_bones); i++) {
            var bone = instance_create(other.x+random_range(-8, 8), other.y+random_range(-24, 8), SkeletonBone);
            bone.direction = 90-(image_xscale*random_range(30, 60));
            bone.speed = random_range(3, 10);
            bone.image_speed = 0;
            bone.image_index = i;
            if (i == 5) bone.image_angle = 130;
        }
    }
    
    attacked = true;
}
